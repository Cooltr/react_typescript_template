{
  "extends": [
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ],
    "ordered-imports": false
  },
  "rules": {
    "named-imports-order": "any",
    "ordered-imports": [false],
    "member-ordering": [false],
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false,
    "jsx-boolean-value": ["never"]
  }
}